{
  "name": "orchestrator",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:frontend": "node scripts/propagate-env.js && cd src/frontend && npm run build",
    "build:backend": "tsc",
    "copy:frontend": "node scripts/copy-frontend.js",
    "build:all": "npm run build:frontend && npm run build:backend && npm run copy:frontend",
    "propagate-env": "node scripts/propagate-env.js",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production npm run build:all && npm start",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:frontend": "cd src/frontend && npm run dev",
    "clean": "rm -rf dist && rm -rf src/frontend/dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:all": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .prettierignore"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@stacks/encryption": "^7.1.0",
    "@stacks/network": "^7.0.2",
    "@stacks/profile": "^7.1.0",
    "@stacks/transactions": "^7.1.0",
    "@stacks/wallet-sdk": "^7.1.2",
    "@types/uuid": "^10.0.0",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.1",
    "redis": "^5.5.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
